#include <iostream>

using namespace std;
double numer;
string dane;
string dane2;

int main()
{
    cout << "Witaj!" << endl;
    cout << "Podaj swoj numer (bez spacji) "<< endl;
    cin >> numer;
    cout << "Podaj imie i nazwisko ";
    cin >> dane;
    cin >> dane2;
    cout << "Twoje imie to: ";
    cout << dane;
    cout << "   A twoj numer telefonu to: ";
    cout << numer;

    return 0;
}
